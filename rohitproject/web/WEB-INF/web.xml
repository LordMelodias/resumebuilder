        <servlet-name>ViewResumeServlet</servlet-name>
        <url-pattern>/ViewResumeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>view_resume</servlet-name>
        <url-pattern>/view_resume</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
